{
    "name": "class-explorer",
    "displayName": "Class Explorer",
    "description": "Show current source file Class hierarchy. Right now PHP only. In near future i will make support TypeScript, JavaScript, C#",
    "version": "0.0.1",
    "publisher": "Lootero4eg",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:nodeDependencies",
        "onView:ftpExplorer",
        "onView:jsonOutline",
        "onView:classExplorer"
    ],
    "main": "./out/src/extension",
    "icon": "media/dep.png",
    "contributes": {
        "views": {
            "explorer": [{
                "id": "classExplorer",
                "name": "Class Explorer"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.1.4",
        "vscode": "^1.0.0",
        "@types/node": "*"
    },
    "dependencies": {
        "jsonc-parser": "^0.4.2",
        "ftp": "^0.3.10"
    }
}